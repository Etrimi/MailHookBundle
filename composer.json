{
    "name": "etrimi/mailhookbundle",
    "type": "symfony-bundle",
    "description": "A bundle to catch API webhook from differents mail service",
    "keywords": ["symfony", "bundle", "mail", "api", "webhook", "hook", "mandrill", "mailjet", "sparkpost", "mailgun"],
    "homepage": "http://github.com/ScullWM/MailHookBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Perez",
            "email": "thomas@scullwm.com"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "symfony/framework-bundle": "~2.8|~3.0|~4"
    },
    "require-dev": {
        "atoum/atoum": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Swm\\Bundle\\MailHookBundle\\": "" },
        "exclude-from-classmap": ["/tests/"]
    }
}
